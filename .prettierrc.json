{
   "singleQuote": true,
   "jsxSingleQuote": true,
   "tabWidth": 3,
   "trailingComma": "es5",
   "importOrder": [
      "^(react|next?/?([a-zA-Z/]*))$",
      "<THIRD_PARTY_MODULES>",
      "^@/shared/(.*)$",
      "^@/features/(.*)$",
      "^@/(.*)$",
      "^[./]"
   ],
   "importOrderSeparation": true,
   "importOrderSortSpecifiers": true,
   "plugins": [
      "@trivago/prettier-plugin-sort-imports",
      "prettier-plugin-tailwindcss"
   ]
}
